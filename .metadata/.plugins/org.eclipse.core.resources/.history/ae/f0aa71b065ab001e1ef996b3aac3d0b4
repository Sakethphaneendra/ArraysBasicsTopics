package Basics;
import java.util.*;

public class findUnique {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		System.out.print("Enter the size of array : ");
		int n = in.nextInt();
		int[] arr = new int[n];
		
		for(int i=0;i<n;i++) {
			 arr[i]=in.nextInt();
		}
		
		System.out.print(" The Array is : "  + Arrays.toString(arr));
		
		for(int i =0 ;i<n;i++) {
			for(int j=i+1;j<n;j++) {
				if(arr[i]==arr[j]) {
					arr[i] = 0;
					arr[j] = 0;
				}
			}
			if(arr[n]!= 0) {
				System.out.println(" The Unqiue Element is [" +arr[i] +"]" );
			}
			
		}
			
	}

}
