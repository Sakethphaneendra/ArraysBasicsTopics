package Basics;
import java.util.*;

public class findUnique {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		System.out.print("Enter the size of array : ");
		int n = in.nextInt();
		int[] arr = new int[n];
		
		for(int i=0;i<n;i++) {
			 arr[i]=in.nextInt();
		}
		
		System.out.print(" The Array is : "  + Arrays.toString(arr));
		
		
		for(int i : arr) {
			for(int j : arr) {
				if(arr[i]==arr[j] && i!=0) {
					arr[i] = 0;
					arr[j] = 0;
				}
			}
		}
		
		for(int i :arr) {
			if(arr[i]!= 0) {
				System.out.println(" The Unqiue Element is [ " +arr[i] +" ]" );
			}
		}
		

		
	}

}
