package Basics;
import java.util.*;

public class findUnique {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] array = {20,20,2,7,3,7,3};
		
		for(int i=0;i<array.length;i++) {
			for(int j=i+1;j<array.length;j++) {
				
				if(i!=j) {
					if(array[i] == array[j]) {
						break;
					}
					else {
						System.out.print(array[i]);
					}
				}
				
			}
			
		}
			
			
	}

}
