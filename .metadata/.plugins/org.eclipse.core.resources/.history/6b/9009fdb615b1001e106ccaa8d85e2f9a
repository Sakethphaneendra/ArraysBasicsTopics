package Basics;

public class Sort01 {

	public static void main(String[] args) {
		
		int arr[] = {2,4,2,5,13};
		
		int len = arr.length;
		
		for(int i=0;i<len;i++){
			int p1 =i;
			int p2 = len-p1;
			System.out.println(p1 + " " +p2);
			
		}
		
