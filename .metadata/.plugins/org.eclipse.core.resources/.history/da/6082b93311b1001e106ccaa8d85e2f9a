package Basics;

public class PairSum {

	public static void main(String[] args) {
		
		int arr[] = {2,4,6,2,7,5,2,3,2,8,3,4,1,4,9,3};
		int x=8;

		for(int i=0;i<arr.length;i++) {
			for(int j=1+1;j<arr.length;j++) {
				if(arr[i]+arr[j]==x) {
					System.out.println("( " +arr[i]+" ," +arr[j]+ " )" );
					arr[i]=0;
					arr[j]=0;
					break;
				}
			}
		}
	}

}
